                            <Grid.RowDefinitions>
                                <RowDefinition Height="60"/>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="10"/>
                            </Grid.RowDefinitions>
                            <Button Grid.Row="0" Grid.Column="0" Margin="10"
						Command="{Binding ShowNewAnodizingProcessProfileCommand}"
						Content="Открыть смену" HorizontalAlignment="Left"  
						Style="{StaticResource controlButtonLightGray}" />
                            <TextBox  x:Name="txtIdAnodizingProcessProfile" Text="{Binding IdAnod, UpdateSourceTrigger=PropertyChanged}"  Grid.Row="0" Grid.Column="1" Visibility="Hidden"/>
                            <TextBlock Grid.Row="2" Text="Данные по процессу анодирования по сменам:" FontSize="14" Margin="5" Background="Transparent" FontWeight="Medium" FontFamily="Montserrat" Foreground="White" />
                            <dxg:GridControl  Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="3" x:Name="gridControlAnodizingProcessProfile" EnableSmartColumnsGeneration="True" ItemsSource="{Binding AnodizingProcessProfileGeneral}" Margin="5" >
